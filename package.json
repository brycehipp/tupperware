{
  "name": "nullshield",
  "version": "0.5.0",
  "description": "A package containing Option and Result types",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run clean && tsc && nyc mocha",
    "doc-build": "typedoc --module commonjs --target ES5 --out doc src/",
    "prepublishOnly": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "find ./ -maxdepth 2 -type f -name \"*.js\" -delete & find ./ -maxdepth 2 -type f -name \"*.d.ts\" -delete & find ./ -maxdepth 2 -type f -name \"*.js.map\" -delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwburns/nullshield.git"
  },
  "author": "Ian Burns <iwburns8@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.1"
  },
  "bugs": {
    "url": "https://github.com/iwburns/nullshield/issues"
  },
  "homepage": "https://github.com/iwburns/nullshield#readme"
}
